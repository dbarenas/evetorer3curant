    * _L_i_s_t_ _o_f_ _c_o_s_m_e_t_i_c_ _i_n_g_r_e_d_i_e_n_t_s
    * _M_a_k_e_u_p_ _b_r_u_s_h
    * _M_o_u_l_a_g_e
    * _P_e_r_m_a_n_e_n_t_ _m_a_k_e_u_p
    * _S_o_c_i_e_t_y_ _o_f_ _C_o_s_m_e_t_i_c_ _C_h_e_m_i_s_t_s
