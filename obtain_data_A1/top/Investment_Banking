                # _2_._4_._2_ _T_e_c_h_n_o_l_o_g_y
          o _2_._5_ _O_t_h_e_r_ _b_u_s_i_n_e_s_s_e_s
    * _3_ _I_n_d_u_s_t_r_y_ _p_r_o_f_i_l_e
          o _3_._1_ _G_l_o_b_a_l_ _s_i_z_e_ _a_n_d_ _r_e_v_e_n_u_e_ _m_i_x
          o _3_._2_ _T_o_p_ _1_0_ _b_a_n_k_s
    * _4_ _F_i_n_a_n_c_i_a_l_ _c_r_i_s_i_s_ _o_f_ _2_0_0_8
    * _5_ _C_r_i_t_i_c_i_s_m_s
          o _5_._1_ _C_o_n_f_l_i_c_t_s_ _o_f_ _i_n_t_e_r_e_s_t
          o _5_._2_ _C_o_m_p_e_n_s_a_t_i_o_n
    * _6_ _S_e_e_ _a_l_s_o
    * _7_ _E_x_t_e_r_n_a_l_ _l_i_n_k_s
    * _8_ _F_u_r_t_h_e_r_ _r_e_a_d_i_n_g
